# StudentService-deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata: 
  name: studentapp-v1
spec: 
  replicas: 1
  selector:
    matchLabels:
      app: studentapp
      version: "v1"
  template:
    metadata: 
      labels:
        app: studentapp
        version: "v1"
    spec: 
      containers:
      - name : studentapp
        image: vikrantcontainerregistry.azurecr.io/studentservice:v1
        ports:
        - containerPort: 8900
          name: https
---
# StudentService-deployment.yaml